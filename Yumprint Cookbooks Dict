            "Salmon Coconut Milk Bisque": {
                "image_url": "https://images.yumprint.com/p1yumprintyd/YDH2X34MJGWD4ENeGZJ84bYqxLt5CQdT9IF5qbBDzGJukxmkHevL3sPejCmUvRuc",
                "recipe_id": "o0a",
                "source_url": "http://recipes.carlosvelez.net/dinner/salmon-coconut-milk-bisque/"
            },
            "details": {
                "ingredient": [
                    "2 cloves garlic",
                    "1/2 cup roasted red pepper",
                    "1 Leek",
                    "4 pinches red pepper flakes",
                    "2 cups salmon, grilled, separated into bite-sized chunks",
                    "1 32 oz can veggie broth, unsalted",
                    "1 13.5 oz can Coconut milk",
                    "1 tsp smoked paprika",
                    "1-2 stalks lemongrass",
                    "1/2 cup parsley"
                ],
                "method": [
                    "Prepare garlic, roasted red pepper, leeks, grilled salmon and red pepper flakes. Divide into two portions",
                    "Saute 1 portion of above ingredients in pot with oil of choice (I used grapeseed oil), about 5 minutes",
                    "Add veggie broth, coconut milk to pot and bring to boil.",
                    "Reduce heat and simmer for 15 minutes, stirring frequently",
                    "Add \u00bd of salmon bits to pot and blend with immersion blender (alternatively use regular blender and put back in pot)",
                    "Add lemongrass, paprika, and 2nd portion of prepared garlic, roasted red pepper, leeks, and red pepper flakes",
                    "Simmer for 10 minutes, stirring frequently",
                    "Add parsley and remaining salmon until heated through",
                    "Serve and garnish with parsley and paprika"
                ]
            }
